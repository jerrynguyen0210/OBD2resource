        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">8.0dip</item>
        <item name="android:layout_weight">36.0</item>
    </style>
    <style name="CanSettings.Col2" parent="@style/CanSettings">
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">64.0</item>
    </style>
    <style name="CanSettingsBTREdit">
